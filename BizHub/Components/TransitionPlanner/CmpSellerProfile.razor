<div class="seller-profile-wrp">
    <div class="profile-contact-sections">
        <div class="row">
            <div class="col-md-12">
                <div class="img-wrap">
                    <h3 class="d-inline-block pl-3">Seller Profile</h3>
                    <img class="seller-profile-avatar mb-0" src="/images/Seller_Profile_Icon_Ring.png">
                </div>
            </div>
        </div>

        <div class="seller-form">
            <!--start-->
            <div class="row">
                <div class="col-md-12">
                    <div class="csinfo-form p-4">
                        <div class="row">
